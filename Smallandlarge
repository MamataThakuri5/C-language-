#include<stdio.h>

int main()
{
int a,b,c,d;
printf("enter the four number:");
scanf("%d%d%d%d",&a,&b,&c,&d);
int big=a>b&& b>c&&c>d?a:((b>c&&c>d)?b:(c>d)?c:d);
    printf("big:%d\n",big);
    
    int small=a<b&&b<c&&c<d?a:((b<c&&c<d)?b:(c>d)?c:d);
    printf ("small:%d\n",small);
    
    return 0;
}
